<script>
function toggleStart(){document.getElementById('start-menu').classList.toggle('open');}
function showContext(e){const m=document.getElementById('context-menu');m.style.left=e.clientX+'px';m.style.top=e.clientY+'px';m.style.display='block';document.onclick=()=>m.style.display='none';}
function bsod(){document.getElementById('bsod').style.display='flex';setTimeout(()=>location.reload(),8000);}
function ransomware(){document.getElementById('ransomware').style.display='flex';for(let i=0;i<30;i++)setTimeout(()=>alert('VIRUS!!!'),i*200);setTimeout(()=>window.location='https://www.youtube.com/watch?v=dQw4w9WgXcQ',4000);}

function openApp(id){document.getElementById(id).classList.add('open');}
function closeApp(id){document.getElementById(id).classList.remove('open');}

// Clock
setInterval(()=>{const d=new Date();document.getElementById('clock').innerHTML=d.toLocaleTimeString([],{hour:'2-digit',minute:'2-digit'})+'<br>'+d.toLocaleDateString();},1000);

// Paint
const canvas=document.getElementById('paintCanvas');if(canvas){const ctx=canvas.getContext('2d');let drawing=false;canvas.onmousedown=()=>drawing=true;canvas.onmouseup=()=>drawing=false;canvas.onmousemove=e=>{if(drawing){ctx.fillStyle=document.getElementById('colorPicker').value;ctx.fillRect(e.offsetX-5,e.offsetY-5,10,10);}};} 

// CMD
function runCommand(cmd){
  const output=document.getElementById('cmd-output');
  output.innerHTML += cmd + '<br>';
  if(cmd==='help') output.innerHTML += 'dir, rickroll, matrix, bsod, clear<br>';
  if(cmd==='rickroll') window.open('https://www.youtube.com/watch?v=dQw4w9WgXcQ');
  if(cmd==='bsod') bsod();
  if(cmd==='clear') output.innerHTML = 'Microsoft Windows [Version 10.0.99999]<br><br>C:\\Users\\Guest>';
  output.innerHTML += '<br>C:\\Users\\Guest>';
  output.scrollTop = output.scrollHeight;
}

// Dragging
document.querySelectorAll('.title-bar').forEach(bar=>{bar.onmousedown=e=>{if(e.target.tagName==='BUTTON')return;let win=bar.parentNode;let shiftX=e.clientX-win.getBoundingClientRect().left;let shiftY=e.clientY-win.getBoundingClientRect().top;document.onmousemove=e=>{win.style.left=(e.clientX-shiftX)+'px';win.style.top=(e.clientY-shiftY)+'px';};document.onmouseup=()=>document.onmousemove=null;};});
</script>
